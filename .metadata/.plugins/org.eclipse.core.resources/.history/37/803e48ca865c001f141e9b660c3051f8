package oops;

public class Calculator {
	
		int add(int n1 , int n2) {
		return n1+n2;
	}
		int add(int n1, int n2, int n3) {
			return n1+n2+n3;
		}

	public static void main(String[] args) {
		
		Calculator obj = new Calculator();
		int r1= obj.add(2, 4);
		int r2= obj.add(2, 3, 4);
		System.out.println(r1);
		
	}

}
