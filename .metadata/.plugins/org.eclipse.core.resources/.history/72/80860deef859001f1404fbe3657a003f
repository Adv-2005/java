package lab3;

public class volume {
		public static void main(String args[]) {
			box b1= new box();
			box b2= new box();
			b1.width= 10;
			b1.height=20;
			b1.length=30;
			
			b2.width= 20;
			b2.height=40;
			b2.length=80;
			
			int v1;
			int v2;
			v1= b1.width * b1.length* b1.width;
			v2= b2.width * b2.length* b2.width;
			
			System.out.println("Volume1: " + v1);
			System.out.println("Volume2: " + v2);
		}
		
	}


